<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lista de Defensores</title>
</head>
<body>

    <table style="border: solid 2px #000;">
        <h1>Defensores Seleccion Argentina</h1>
        <% defensores.forEach(function(defensa){ %>
            <tr>
                <th><%= defensa.nombre %></th>
                <th><%= defensa.apellido %></th>
                <th><%= defensa.edad %></th>
                <th><%= defensa.equipo %></th>
                <th><%= defensa.energia %></th>
                
                
            </tr>
        <% }); %>
      
    </table>

    <button>
        <a href="/">Volver Home</a>
    </button>
    
</body>
</html>